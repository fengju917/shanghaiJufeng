<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowActionBar">false</item>
        <item name="android:windowTitleSize">79dip</item>
    </style>
    
	<!-- mainfirstlayout.xml imageView and textView styles. -->
	<style name="imageViewStyle">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:layout_marginTop">13dip</item>
	    <item name="android:layout_marginLeft">30dip</item>
	</style>
	<style name="imageViewStyle_800x600">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	</style>
	
    <style name="textViewStyle">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:gravity">center</item>
	    <item name="android:paddingBottom">5dip</item>
	    <item name="android:textSize">20sp</item>
    </style>
    
    <style name="imageViewStyle_1024x768">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:layout_marginTop">16.64dip</item>
	    <item name="android:layout_marginLeft">38.4dip</item>
	</style>
	
    <style name="textViewStyle_1024x768">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:gravity">center</item>
	    <item name="android:paddingBottom">6.4dip</item>
	    <item name="android:textSize">25.6sp</item>
    </style>
    
    <style name="imageViewStyle_1280x800">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:layout_marginTop">18.2dip</item>
	    <item name="android:layout_marginLeft">60dip</item>
	</style>
	
    <style name="textViewStyle_1280x800">
        <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:gravity">center</item>
	    <item name="android:paddingBottom">7.2dip</item>
	    <item name="android:textSize">25.6sp</item>
    </style>
    
        <style name="Text">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Text.Title_Button" parent="@style/Text">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
    </style>
    
    <style name="Text.Title" parent="@style/Text">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
    </style>
    
    <style name="settingsImageStyle">
	    <item name="android:layout_width">80dip</item>
	    <item name="android:layout_height">80dip</item>
	    <item name="android:layout_marginTop">40dip</item>
	    <item name="android:layout_marginLeft">30dip</item>
	</style>
	
    <style name="Transparent">   
		<item name="android:windowBackground">@color/transparent</item>   
		<item name="android:windowIsTranslucent">true</item>   
		<item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>   
	</style>
	
    <style name="FullTransparent">   
		<item name="android:windowBackground">@color/fulltransparent</item>   
		<item name="android:windowIsTranslucent">true</item>   
		<item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>   
	</style>  
	
    <style name="DialogStyle" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar"> 
        <item name="android:windowBackground">@color/dialogbackground</item> 
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    
    <style name="ProgressDialogStyle" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar"> 
        <item name="android:windowBackground">@color/dialogbackground</item> 
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:backgroundDimEnabled">false</item> 
        <item name="android:textSize">24sp</item>
    </style>
	
    <!-- add by txb -->
    <style name = "term_param_setting_style">
		<item name = "android:textSize">30sp</item>        
		<item name = "android:textColor">#000000</item>        
		<item name = "android:layout_marginLeft">20dip</item>        
		<item name = "android:layout_marginTop">15dip</item>        
		<item name = "android:layout_marginBottom">15dip</item>        
    </style>
    <style name = "term_param_setting_style_1024x768">
		<item name = "android:textSize">38.4sp</item>        
		<item name = "android:textColor">#000000</item>        
		<item name = "android:layout_marginLeft">26.5dip</item>        
		<item name = "android:layout_marginTop">19.2dip</item>        
		<item name = "android:layout_marginBottom">19.2dip</item>        
    </style>
    
    <style name = "term_param_setting_itemlinerlayout_style">
		<item name = "android:layout_marginTop">5dip</item>
		<item name = "android:layout_marginBottom">5dip</item>
		<item name = "android:paddingTop">5dip</item>        
		<item name = "android:paddingBottom">5dip</item>        
    </style>
    <style name="FullHeightDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
	<style name="editTextBg" >
        <item name="android:background">@drawable/inputback</item>
    </style>
    <style name="inputBg" >
        <item name="android:background">@drawable/inputshape</item>
        <item name="android:paddingLeft">3dip</item>
    </style>
    <style name="linerlayoutbg" >
        <item name="android:background">@drawable/item_linerlayout_bg</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:paddingBottom">5dip</item>
        <item name = "android:layout_marginTop">5dip</item>
		<item name = "android:layout_marginBottom">5dip</item>
		<item name = "android:gravity">left|center</item>
    </style>
     <style name="imageview_box_style" >
        <item name="android:background">@drawable/choose_no</item>
    </style>
</resources>
